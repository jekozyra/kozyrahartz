<h1>Listing news_entries</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Entry</th>
  </tr>

<% @news_entries.each do |news_entry| %>
  <tr>
    <td><%=h news_entry.title %></td>
    <td><%=h news_entry.entry %></td>
    <td><%= link_to 'Show', news_entry %></td>
    <td><%= link_to 'Edit', edit_news_entry_path(news_entry) %></td>
    <td><%= link_to 'Destroy', news_entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New news_entry', new_news_entry_path %>
