<h1>Editing news_entry</h1>

<% form_for(@news_entry) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :entry %><br />
    <%= f.text_area :entry %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @news_entry %> |
<%= link_to 'Back', news_entries_path %>